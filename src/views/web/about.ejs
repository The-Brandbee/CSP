<!DOCTYPE HTML>
<html>
    <%-include('head.ejs')%>
<body>
    <%-include('header.ejs')%>
<div class="loader-main">
	<div class="lds-ellipsis"><div></div><div></div><div></div></div>
	</div>
<div class="inner-banner">
	<div id="myCarousel" class="carousel slide" data-ride="carousel">
  <!-- Indicators -->
  <!-- <ol class="carousel-indicators">
    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
    <li data-target="#myCarousel" data-slide-to="1"></li>
    <li data-target="#myCarousel" data-slide-to="2"></li>
  </ol> -->

  <!-- Wrapper for slides -->
  <div class="carousel-inner">
    <div class="item active">
      <img src="/static/images/banner-img1.jpg" class="img-responsive"/>
    </div>
  </div>
</div>
</div>
<div class="about-main">
	<div class="container">
		<div class="row">
			<div class="col-lg-5 col-md-8 col-sm-12">
				<h1>Why Metaguise :-</h1>
                    <%-about[0].whymetaguisedesc?about[0].whymetaguisedesc:''%>
			</div>
			<div class="col-lg-offset-1 col-lg-6 col-md-4 col-sm-12">
				<img src="<%=about[0].whymetaguiseimage?about[0].whymetaguiseimage.replace('public', '/static'):''%>"/>
			</div>
		</div>	
	</div>
</div>
<div class="aboutseconddiv">
	<div class="container">
		<div class="row">
			<div class="col-lg-5 col-sm-offset-1  col-sm-6">
				<img src="<%=about[0].metaguise_exp_image1?about[0].metaguise_exp_image1.replace('public', '/static'):''%>"/>
			</div>
			<div class="col-lg-6 col-sm-6">
				<h2>The Metaguise Exprience</h2>
                <%-about[0].metaguise_exp_desc?about[0].metaguise_exp_desc:""%>
				<ul>
                    <%for (var i=0; i < aboutfeature.length; i++) { 
                        if(aboutfeature[i].page.includes("aboutuspage")){%>
                        <li>
                            <img src="<%=aboutfeature[i].image?aboutfeature[i].image.replace('public', '/static'):''%>"/>
                            <p><%-aboutfeature[i].name?aboutfeature[i].name:""%></p>
                        </li>
                    <%}}%>
				</ul>
			</div>
		</div>
	</div>
</div>
<div class="attheratemetaguise">
	<div class="container">
		<div class="row">
			<div class="col-sm-12">
				<h3>@metaguise</h3>
				<p>Let's get in touch! Follow us on social media to see <br> all the latest creations</p>
				<ul>
					<li><img src="/static/images/attheratemetaguise-img1.jpg" class="inner-img" /></li>
					<li><img src="/static/images/attheratemetaguise-img2.jpg" class="inner-img"/></li>
					<li><img src="/static/images/attheratemetaguise-img3.jpg" class="inner-img"/></li>
					<li><img src="/static/images/attheratemetaguise-img4.jpg" class="inner-img"/></li>
					<li><img src="/static/images/attheratemetaguise-img5.jpg" class="inner-img"/></li>
					<li><img src="/static/images/attheratemetaguise-img6.jpg" class="inner-img"/></li>
				</ul>
			</div>
		</div>
	</div>
</div>
<%-include('footer.ejs')%>
<script type="text/javascript" src="/static/js/jquery-1.12.0.min.js"></script> 
<script src="/static/js/bootstrap.min.js"></script> 
<script type="text/javascript" src="/static/js/owl.carousel.min.js"></script>
<script src="/static/js/aos.js"></script> 
<script type="text/javascript" src="/static/js/custom.js"></script> 
<script type="text/javascript">
	$(document).ready(function() {
		$(document).delegate('.open', 'click', function(event){
			$(this).addClass('oppenned');
			event.stopPropagation();
		})
		$(document).delegate('body', 'click', function(event) {
			$('.open').removeClass('oppenned');
		})
		$(document).delegate('.cls', 'click', function(event){
			$('.open').removeClass('oppenned');
			event.stopPropagation();
		});
	});
</script>
<script type="text/javascript">

  </script>
</body>
</html>